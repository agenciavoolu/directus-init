{
	"name": "directus-extension-app-bundle",
	"version": "1.0.0",
	"type": "module",
	"dependencies": {},
	"directus:extension": {
		"host": "^10.1.4",
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "hook",
				"name": "auth",
				"source": "src/hooks/auth/index.ts"
			}
		]
	},
	"description": "Directus extensions used in this app",
	"icon": "extension",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.1.4",
		"@types/node": "^20.3.2",
		"typescript": "^5.1.3"
	}
}
